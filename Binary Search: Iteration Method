package aryan;
import java.util.*;
public class binary {
	public int binarysearch(int arr[],int n,int target)
	{
		int low=0;
		int high=n-1;
		while(low<=high)
		{
			int mid=(low+high)/2;
			if(target==arr[mid])
				return mid;
			else if(target<mid)
			{
				high=mid-1;
			}
			else
			{
				low=mid+1;
			}
		}
		return -1;
	}
	
public static void main(String[] args) {
	Scanner scanner=new Scanner(System.in);
	int n=scanner.nextInt();
	int arr[]=new int[n];
	for(int i=0;i<n;i++)
	{
		arr[i]=scanner.nextInt();
	}
	int target=scanner.nextInt();
	binary b=new binary();
	int answer=b.binarysearch(arr,n,target);
	if(answer>=0)
	System.out.println("The target element is at place "+answer);
	else
	{
		System.out.println("NOT FOUNDED!!!!");
	}
}
}
