import java.util.*;

 

public class Main {

    public static void main(String[] args) {

    Scanner scanner=new Scanner(System.in);

    int n=scanner.nextInt();

    int arr[]=new int[n];

    int maximum=Integer.MIN_VALUE;

    for(int i=0;i<n;i++)

    {

      arr[i]=scanner.nextInt();

      if(arr[i]>maximum)

      maximum=arr[i];

    }

    int hashing[]=new int[maximum+1];

    for(int i=0;i<n;i++)

    {

      hashing[arr[i]]++;

    }

    int maxele=0;

    int maximum_value=Integer.MIN_VALUE;

    for(int i=0;i<hashing.length;i++)

    {

      if(hashing[i]>maximum_value)

      {

        maximum_value=hashing[i];

        maxele=i;

      }

    }

    int q=scanner.nextInt();

    System.out.println(hashing[q]);

    System.out.println("The highest element is "+maxele);

    System.out.println("The frequency is "+maximum_value);

  }

}
